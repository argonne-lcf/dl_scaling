{
  "train_batch_size": 256,
  "steps_per_print": 10,
  "optimizer": {
    "type": "SGD",
    "params": {
      "lr": 0.1
    }
  },
  "scheduler": {
    "type": "WarmupLR",
    "params": {
      "warmup_min_lr": 0,
      "warmup_max_lr": 0.001,
      "warmup_num_steps": 1000
    }
  },
  "gradient_clipping": 1.0,
  "prescale_gradients": false,
  "fp16": {
      "enabled": false
  },
  "wall_clock_breakdown": false,
  "zero_optimization": {
      "stage": 0
  }
}
